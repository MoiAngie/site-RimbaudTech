<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Rimbaud'Tech | Admin{% endblock %}</title>

        {% block stylesheets %}
        <link rel="shortcut icon" type="image/png" href="/img/favicon_rimbaud-tech.png"/>
        <link href="{{asset('css/css/base-index.css')}}" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Muli&display=swap" rel="stylesheet">
        <link href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" rel="stylesheet"
        integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf"
        crossorigin="anonymous">
        <!-- Core jQuery Script -->
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.js"></script>

        {% endblock %}
    </head>
    <body>
      <!-- backtotop -->

      <button onclick="topFunction()" id="myBtn" title="Retour en haut"></button>

      <!-- Début Navbar -->
      <div class="topnav" id="myTopnav">
        <a href="{{ path('index')}}">
          <img src={{ asset('img/logo-gris.jpg')}} alt="logo Rimbaud'Tech" title="Retour sur le site" height="90" width="279">
        </a>
        <div class="topnav-right">
          <a href="{{path('homeAdmin')}}"><i class="fas fa-home" title="Accueil du panneau admin"></i></a>
          <div class="dropdown">
            <button class="dropbtn">Modifications</button>
            <div class="dropdown-content">
              <a href="{{path('portailContent')}}">Incubés, coworkers et personnel</a>
              <a href="{{path('modify-tarifs')}}">Les tarifs</a>
              <a href="{{path('portailAdmin')}}">Les administrateurs</a>
            </div>
          </div>
          <a href="{{path('portailBooking')}}">Réservations</a>
          <a href="{{path('portailArticles')}}">Articles</a>
          <a href="{{path('security_logout')}}">Déconnexion</a>
          <a href="javascript:void(0);" class="icon" onclick="myFunction()">&#9776;</a>
        </div>
      </div>
      <!-- Fin Navbar -->
      {% block body %}
      {% endblock %}

        {% block javascripts %}
        <script src="{{asset('js/base.js')}}">
        </script>
      {% endblock %}
    </body>
</html>
