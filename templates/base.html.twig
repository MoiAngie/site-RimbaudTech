<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link href="{{asset('css/css/base.css')}}" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Muli&display=swap" rel="stylesheet">
        <link href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" rel="stylesheet"
        integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf"
        crossorigin="anonymous">
        {% block stylesheets %}
        {% endblock %}
    </head>
    <body>
      <!-- Début Navbar -->
      <div class="topnav" id="myTopnav">
        <a href="#home">
          <img src={{ asset('img/logo-RT.png')}} alt="logo Rimbaud'Tech" title="logo Rimbaud'Tech" height="90" width="279">
        </a>
        <div class="topnav-right">
          <div class="dropdown">
            <button class="dropbtn">Services</button>
            <div class="dropdown-content">
              <a href="{{ path('incubation')}}">Incubation</a>
              <a href="{{ path('coworking')}}">Coworking</a>
              <a href="{{ path('location')}}">Location de salles</a>
            </div>
          </div>
          <a href="{{ path('galerie-incubes')}}">Nos incubés</a>
          <a href="{{ path('actu')}}">Actualités</a>
          <a href="{{ path('contact')}}">Contact</a>
          <a href="javascript:void(0);" class="icon" onclick="myFunction()">&#9776;</a>
        </div>
      </div>
      <!-- Fin Navbar -->
        {% block body %}{% endblock %}
        <script src="js/base.js"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
