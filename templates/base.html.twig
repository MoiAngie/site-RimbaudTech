    <body>
      <!--bouton back-to-top-->

      <button onclick="topFunction()" id="myBtn" title="Retour en haut"></button>

      <!-- Début Navbar -->
      <div class="topnav" id="myTopnav">
        <a href="{{ path('index')}}">
          <img src={{ asset('img/logo-RT.png')}} alt="logo Rimbaud'Tech" title="logo Rimbaud'Tech" height="90" width="279">
        </a>
        <div class="topnav-right">
          <div class="dropdown">
            <button class="dropbtn">Services</button>
            <div class="dropdown-content">
              <a href="{{ path('incubation')}}">Incubation</a>
              <a href="{{ path('coworking')}}">Coworking</a>
              <a href="{{ path('location')}}">Location de salles</a>
            </div>
          </div>
            <a href="{{ path('galerie-incubes')}}">Nos incubés</a>
            <a href="{{ path('actu')}}">Actualités</a>
            <a href="{{ path('contact')}}">Contact</a>
            {% if not app.user %}
            <a href="{{ path('rt_security_login')}}">Connexion</a>
            {% endif %}

            {% if app.user %}
            <a href="{{ path('rt_security_profile')}}" class="blue">{{ app.user.username |capitalize }} <i class="fas fa-check"></i> </a>
            {% endif %}
          </div>
          <a href="javascript:void(0);" class="icon" onclick="myFunction()">&#9776;</a>
      </div>
      <!-- Fin Navbar -->
      {% for class, messages in app.flashes %}
          {% for message in messages %}
              <div class="{{ class }}">
                  {{ message }}
              </div>
          {% endfor %}
      {% endfor %}
{% block body %}{% endblock %}

